{
  "root": "apps/ove-app-controller",
  "sourceRoot": "apps/ove-app-controller/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ove-app-controller",
        "main": "apps/ove-app-controller/src/index.js",
        "tsConfig": "apps/ove-app-controller/tsconfig.app.json",
        "assets": ["apps/ove-app-controller/src"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/ove-app-controller/src/environments/environment.js",
              "with": "apps/ove-app-controller/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "ove-app-controller:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ove-app-controller/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ove-app-controller"],
      "options": {
        "jestConfig": "apps/ove-app-controller/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
